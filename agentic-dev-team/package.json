{
  "name": "agentic-dev-team",
  "version": "1.0.0",
  "description": "Comprehensive agentic coding agent team using Claude Code TypeScript SDK",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "test": "jest",
    "lint": "eslint src --ext .ts,.js",
    "typecheck": "tsc --noEmit",
    "setup": "npm run build && npm run setup-claude-config",
    "setup-claude-config": "node dist/setup-claude-config.js"
  },
  "dependencies": {
    "@anthropic/claude-code": "latest",
    "@modelcontextprotocol/sdk": "latest",
    "zod": "^3.23.0",
    "winston": "^3.11.0",
    "dotenv": "^16.3.1",
    "fs-extra": "^11.2.0",
    "yaml": "^2.3.4",
    "chalk": "^5.3.0",
    "inquirer": "^9.2.12"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/jest": "^29.5.8",
    "@types/inquirer": "^9.0.7",
    "@types/fs-extra": "^11.0.4",
    "typescript": "^5.3.0",
    "tsx": "^4.6.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}